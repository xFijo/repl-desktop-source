{
  "name": "replit-desktop",
  "productName": "Replit Desktop",
  "version": "3.0.0-beta2",
  "description": "Desktop version of replit",
  "keywords": [
    "electron",
    "repl.it",
    "Replit",
    "Replit Desktop",
    "Program editor",
    "discord-rpc"
  ],
  "repository": "https://github.com/replit-discord/replit-desktop",
  "license": "Apache-2.0",
  "author": "leon332157&repl-it-discord",
  "main": "main.js",
  "scripts": {
    "build:app:test": "electron-builder -c electron-builder.conf.js --dir",
    "build:dev": "gulp buildDev",
    "build:prod": "gulp buildProd",
    "build:run": "electron --trace-warnings -no-sandbox ./ts-out/",
    "dist": "gulp buildApp",
    "pre-dist": "gulp buildAppPreRelease",
    "start": "gulp buildAndRun",
    "watch": "gulp watchDev",
    "lint:check": "eslint \"**/*.{js,ts,json}\"",
    "lint": "eslint \"**/*.{js,ts,json}\" --fix"
  },
  "dependencies": {
    "discord-rpc": "^3.2.0",
    "electron-context-menu": "^2.5.1",
    "node-fetch": "^2.6.1",
    "semver": "^7.3.5",
    "write-file-atomic": "^3.0.3"
  },
  "devDependencies": {
    "@octokit/types": "^6.13.0",
    "@sucrase/gulp-plugin": "^2.0.0",
    "@types/discord-rpc": "^3.0.5",
    "@types/gulp": "^4.0.8",
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "@types/semver": "^7.3.4",
    "@types/write-file-atomic": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^3.4.0",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-json-editor": "^2.5.5",
    "gulp-terser-js": "^5.2.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "prettier": "^2.2.1",
    "sucrase": "^3.18.1",
    "typescript": "^4.2.4",
    "zip-local": "^0.3.4"
  },
  "email": "leon331257@gmail.com"
}
